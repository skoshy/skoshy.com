{
  "name": "skoshy.com",
  "description": "Stefan's Website",
  "version": "1.0.0",
  "author": "Stefan Koshy",
  "scripts": {
    "preinstall": "npx use-yarn -m 'Please_use_yarn!'",
    "prestart": "npx use-yarn -m 'Please_use_yarn!'",
    "dev": "cross-env NODE_ENV=development next --port 30483",
    "build": "cross-env NODE_ENV=production next build",
    "export": "cross-env NODE_ENV=production next export",
    "start": "cross-env NODE_ENV=production next start --port 30484",
    "serve": "npm run start",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "npm run lint:eslint && npm run lint:prettier && npm run lint:styles",
    "lint:eslint": "eslint '{pages,src}/**/*.{tsx,ts,js,jsx}'",
    "lint:prettier": "prettier --write '{pages,src}/**/*.{tsx,ts,js,jsx}'",
    "lint:styles": "prettier-stylelint --write 'src/**/*.{css,scss,tsx,ts}' ",
    "log": "node scripts/index.js"
  },
  "dependencies": {
    "grommet": "2.14.0",
    "immer": "6.0.3",
    "next": "9.4.4",
    "ramda": "0.27.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-typography": "0.16.19",
    "styled-components": "5.1.1",
    "tailwindcss": "1.5.1",
    "typography": "0.16.19",
    "typography-theme-bootstrap": "0.16.19",
    "use-immer": "0.4.1"
  },
  "devDependencies": {
    "@types/node": "14.0.23",
    "@types/react": "16.9.43",
    "@types/react-dom": "16.9.8",
    "@types/styled-components": "5.1.1",
    "@typescript-eslint/eslint-plugin": "3.6.1",
    "@typescript-eslint/parser": "3.6.1",
    "babel-plugin-root-import": "6.5.0",
    "babel-plugin-styled-components": "1.10.7",
    "cross-env": "7.0.2",
    "eslint": "7.4.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-import-resolver-babel-plugin-root-import": "1.1.1",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-react": "7.20.3",
    "prettier": "2.0.5",
    "prettier-stylelint": "0.4.2",
    "shelljs": "0.8.4",
    "stylelint": "13.3.3",
    "stylelint-config-recommended": "3.0.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-order": "4.0.0",
    "stylelint-processor-styled-components": "1.10.0",
    "typescript": "3.9.7",
    "utility-types": "3.10.0"
  },
  "keywords": [
    "nextjs"
