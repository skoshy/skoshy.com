    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-typography": "0.16.19",
    "sass": "1.32.5",
    "styled-components": "5.2.1",
    "tailwindcss": "2.0.2",
    "typography": "0.16.19",
    "typography-theme-bootstrap": "0.16.19",
    "use-immer": "0.4.2"
  },
